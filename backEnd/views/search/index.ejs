<% let pageTitle="Revour - tourist spot review site" %>
    <!-- body is started here -->
    <%- include('../common/_header.ejs',{pageTitle:pageTitle}) %>
        <style>
            /* Always set the map height explicitly to define the size of the div
         * element that contains the map. */
            #map {
                height: 500px;
                width: 500px;
            }

            /* Optional: Makes the sample page fill the window. */
            html,
            body {
                height: 100%;
                margin: 0;
                padding: 0;
            }
        </style>
        <div id="map"></div>
        <script>
            var map;
            function initMap() {
                map = new google.maps.Map(document.getElementById('map'), {
                    center: { lat: -34.397, lng: 150.644 },
                    zoom: 8
                });
            }
        </script>
        <script
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCHgXgNbWJX6yX-B1wuXV0dAtk0gK2s5Cw&callback=initMap"
            async defer></script>
        </body>

        </html>